<printer id="printer/Brother-PT-P750W">
  <make>Brother</make>
  <model>PT-P750W</model>
  <pcmodel>BRP750W</pcmodel>
  <mechanism>
    <thermal/>
    <!--not "color"-->
    <resolution>
      <dpi>
        <x>180</x>
        <y>360</y>
      </dpi>
    </resolution>
  </mechanism>
  <url>https://www.brother.co.uk/labelling-and-receipts/ptp750w</url>
  <lang>
    <proprietary />
  </lang>
  <autodetect>
    <general>
      <ieee1284>MFG:Brother;CMD:PT-CBP;MDL:PT-P750W;CLS:PRINTER;</ieee1284>
      <commandset>PT-CBP</commandset>
      <description>Brother PT-P750W</description>
      <manufacturer>Brother</manufacturer>
      <model>PT-P750W</model>
    </general>
  </autodetect>
  <functionality>D</functionality>
  <driver>ptouch-pt</driver>
  <!--no "contrib_url"-->
  <comments>
    <en>
    Tape width 3.5mm to 24mm.
    Prints 30mm per second.
    Automatic cutter.
    </en>
  </comments>
  <select>
    <option id="opt/Brother-PTQL-TransferMode" />
    <option id="opt/Brother-PTQL-Resolution">
      <arg_defval>ev/180dpi</arg_defval>
      <enum_val id="ev/180x360dpi" />
      <enum_val id="ev/180dpi" />
    </option>
    <option id="opt/Brother-PTQL-BytesPerLine">
      <enum_val id="ev/16" />
    </option>
    <option id="opt/Brother-PTQL-LabelPreamble" />
    <option id="opt/Brother-PT-LabelRecovery" />
    <option id="opt/Brother-PT-PageSize">
      <arg_defval>ev/tz-24</arg_defval>
      <enum_val id="ev/tz-4" />
      <enum_val id="ev/tz-6" />
      <enum_val id="ev/tz-9" />
      <enum_val id="ev/tz-12" />
      <enum_val id="ev/tz-18" />
      <enum_val id="ev/tz-24" />
      <enum_val id="ev/hs-6" />
      <enum_val id="ev/hs-9" />
      <enum_val id="ev/hs-12" />
      <enum_val id="ev/hs-18" />
      <enum_val id="ev/hs-24" />
    </option>
    <option id="opt/Brother-PTQL-PixelTransfer">
      <enum_val id="ev/RLE" sense="false" />
    </option>
    <option id="opt/Brother-PTQL-AutoCut" />
  </select>
</printer>
